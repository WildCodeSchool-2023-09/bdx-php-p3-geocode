{% extends 'base.html.twig' %}

{% block title %}Geocode - Carte{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link
        rel="stylesheet"
        href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css"
    />
{% endblock %}

{% block body %}
    <main class="map">
        <div id="map" {{ stimulus_controller('map_search') }}
             data-longitude = '{{ town.point.longitude }}'
             data-latitude = '{{ town.point.latitude }}'></div>
    </main>
    <div class="map-modal">
{#        {{ dump(town) }}#}
    </div>
{% endblock %}


